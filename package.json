    "body-parser": "^1.18.3",
    "bonjour": "^3.5.0",
    "bootstrap": "^4.1.3",
    "chalk": "^2.4.1",
    "connect": "^3.6.6",
    "cors": "^2.8.4",
    "date-fns": "^1.29.0",
    "errorhandler": "^1.5.0",
    "es6-promisify": "^6.0.0",
    "ethereumjs-util": "^5.2.0",
    "ethers": "^4.0.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
